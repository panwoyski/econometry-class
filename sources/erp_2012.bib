@article{ ISI:000298985500001,
Author = {Holt, Diane and Barkemeyer, Ralf},
Title = {{Media coverage of sustainable development issues - attention cycles or
   punctuated equilibrium?}},
Journal = {{SUSTAINABLE DEVELOPMENT}},
Year = {{2012}},
Volume = {{20}},
Number = {{1}},
Pages = {{1-17}},
Month = {{JAN-FEB}},
Abstract = {{Over the last two decades there has been a marked change in the way that
   the public perceives environmental issues and the concepts associated
   with sustainable development. Few would argue that this has moved into
   the mainstream of public consciousness. Coverage of sustainability
   related concepts within the media, as illustrated by a sample of 112
   worldwide newspapers, shows a clear upward trajectory. Taking the
   example of climate change and sustainable development, this paper
   explores this increasing coverage, and arguably awareness, of
   sustainability concepts, and considers parallels with the concepts of
   punctuated equilibrium and issue-attention cycles. Copyright (C) 2010
   John Wiley \& Sons, Ltd and ERP Environment.}},
