@article{ ISI:000298945400002,
Author = {Melo, Tiago and Garrido-Morgado, Alvaro},
Title = {{Corporate Reputation: A Combination of Social Responsibility and
   Industry}},
Journal = {{CORPORATE SOCIAL RESPONSIBILITY AND ENVIRONMENTAL MANAGEMENT}},
