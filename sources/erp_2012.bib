@article{ ISI:000301915200012,
Author = {Salmeron, Jose L. and Lopez, Cristina},
Title = {{Forecasting Risk Impact on ERP Maintenance with Augmented Fuzzy
   Cognitive Maps}},
Journal = {{IEEE TRANSACTIONS ON SOFTWARE ENGINEERING}},
