@article{ ISI:000348573500003,
Author = {Lozano, Rodrigo},
Title = {{A Holistic Perspective on Corporate Sustainability Drivers}},
Journal = {{CORPORATE SOCIAL RESPONSIBILITY AND ENVIRONMENTAL MANAGEMENT}},
