@article{ ISI:000348565800004,
Author = {Kim, Yunhee},
Title = {{Environmental, Sustainable Behaviors and Innovation of Firms During the
   Financial Crisis}},
Journal = {{BUSINESS STRATEGY AND THE ENVIRONMENT}},
