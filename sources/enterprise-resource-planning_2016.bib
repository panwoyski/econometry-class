@inproceedings{ ISI:000393215400008,
Author = {Colakovic, Amar},
Editor = {{Obralic, M and Mekic, E}},
Title = {{``IMPLEMENTATION OF AN ENTERPRISE RESOURCE PLANNING (ERP) AND ITS EFFECT
