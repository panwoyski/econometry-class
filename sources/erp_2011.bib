@article{ ISI:000287344000002,
Author = {Quental, Nuno and Lourenco, Julia M. and da Silva, Fernando Nunes},
Title = {{Sustainable Development Policy: Goals, Targets and Political Cycles}},
Journal = {{SUSTAINABLE DEVELOPMENT}},
Year = {{2011}},
Volume = {{19}},
Number = {{1}},
