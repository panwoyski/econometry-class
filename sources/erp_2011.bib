@article{ ISI:000289733300001,
Author = {Ozaki, Ritsuko},
Title = {{Adopting Sustainable Innovation: What Makes Consumers Sign up to Green
   Electricity?}},
Journal = {{BUSINESS STRATEGY AND THE ENVIRONMENT}},
Year = {{2011}},
Volume = {{20}},
Number = {{1}},
Pages = {{1-17}},
Month = {{JAN}},
Abstract = {{This paper investigates what encourages consumers to adopt a green
   electricity tariff. When people decide to adopt an innovation, such as
   green electricity, they consider not only functionality, usability,
   costs and intended outcomes, but also what the innovation means to them,
   for example, the way it reflects their identity, image, memberships,
   values and norms. The study reviews the theoretical frameworks of
   innovation adoption and consumption, and cognitive and normative
   behaviour, relevant to consumer adoption of pro-environmental
   innovations, and develops a research framework. Through focus group
   discussions, a questionnaire survey with 103 respondents and an
   interview with 10 people, the study finds that consumers sympathetic to
   environmental issues do not necessarily adopt green electricity. This is
   due to lack of strong social norms and personal relevance, inconvenience
   of switching, uncertainty about the quality of green electricity and
   lack of accurate information. The implications of these findings for
   strategy, policy and future research are explored. Copyright (C) 2009
   John Wiley \& Sons, Ltd and ERP Environment.}},
Publisher = {{WILEY PERIODICALS, INC}},
Address = {{COMMERCE PLACE, 350 MAIN STREET, MALDEN, MA 02148-529 USA}},
