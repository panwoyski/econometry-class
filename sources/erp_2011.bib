@article{ ISI:000299960000006,
Author = {Bernroider, Edward W. N. and Sudzina, Frantisek and Pucihar, Andreja},
Title = {{Contrasting ERP Absorption Between Transition and Developed Economies
   From Central and Eastern Europe (CEE)}},
Journal = {{INFORMATION SYSTEMS MANAGEMENT}},
Year = {{2011}},
Volume = {{28}},
Number = {{3, SI}},
Pages = {{240-257}},
Abstract = {{This article investigates Enterprise Resource Planning absorption in
   transition and developed economies in Central and Eastern Europe. Using
   absorptive capacity theory and data envelopment analysis, we view
   organizational transformation in Enterprise Resource Planning absorption
   as an economic production process. Despite converging Enterprise
   Resource Planning saturation levels, the data identifies gaps in
   absorption levels and performance. Organizations in transition face
   greater challenges, engage more in phased Enterprise Resource Planning
   absorptions, and expect higher levels of external support.}},
