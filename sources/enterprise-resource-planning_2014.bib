@article{ ISI:000336669800039,
Author = {Tao, Fei and Zuo, Ying and Xu, Li Da and Lv, Lin and Zhang, Lin},
Title = {{Internet of Things and BOM-Based Life Cycle Assessment of Energy-Saving
   and Emission-Reduction of Products}},
Journal = {{IEEE TRANSACTIONS ON INDUSTRIAL INFORMATICS}},
