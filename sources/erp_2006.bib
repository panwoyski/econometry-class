@article{ ISI:000237500400002,
Author = {Li, Ling and Zhao, Xiping},
Title = {{Enhancing competitive edge through knowledge management in implementing
   ERP systems}},
Journal = {{SYSTEMS RESEARCH AND BEHAVIORAL SCIENCE}},
Year = {{2006}},
Volume = {{23}},
Number = {{2}},
Pages = {{129-140}},
Month = {{MAR-APR}},
Abstract = {{Using data from 170 manufacturing companies, we identified five
