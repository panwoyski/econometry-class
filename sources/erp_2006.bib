@article{ ISI:000236118600005,
Author = {Campling, L and Rosalie, M},
Title = {{Sustaining social development in a small island developing state? The
   case of Seychelles}},
Journal = {{SUSTAINABLE DEVELOPMENT}},
Year = {{2006}},
Volume = {{14}},
Number = {{2}},
Pages = {{115-125}},
Month = {{APR}},
Abstract = {{Since the 1970s there has been a significant interest in small island
   developing states (SIDS). Since the 1990s the vast majority of this
   focus has been on economic and environmental `vulnerabilities'. This
   paper reaffirms the importance of `the social' when analysing the
   `vulnerabilities' of SIDS through a country case study of Seychelles in
   the context of its SIDS status. It outlines Seychelles' demographic
   processes and macroeconomic volatilities and discusses its
   post-independence social development achievements. This paper provides
   critical analysis of how these achievements have been attained and
   considers the extent to which the present levels are sustainable. It
   argues that significant social gains are under threat from a range of
   domestic and international forces, potentially destabilizing the main
   pillar of the economy, tourism. The paper concludes that while the
   economic and environmental vulnerabilities of SIDS are of conceptual
   importance, reaffirming the centrality of the social is vital to
   understanding the potential causes and consequences of these
   vulnerabilities. Copyright (c) 2006 John Wiley \& Sons, Ltd and ERP
   Environment.}},
