@article{ ISI:000242264600013,
Author = {Zhang, David Zhengwen and Anosike, Anthony Ikechukwu and Lim, Ming Kim
   and Akanle, Oluwaremilekun Mowanuola},
Title = {{An agent-based approach for e-manufacturing and supply chain integration}},
Journal = {{COMPUTERS \& INDUSTRIAL ENGINEERING}},
