@article{ ISI:000275074600007,
Author = {Morris, Michael G. and Venkatesh, Viswanath},
Title = {{JOB CHARACTERISTICS AND JOB SATISFACTION: UNDERSTANDING THE ROLE OF
   ENTERPRISE RESOURCE PLANNING SYSTEM IMPLEMENTATION}},
Journal = {{MIS QUARTERLY}},
Year = {{2010}},
Volume = {{34}},
Number = {{1}},
Pages = {{143-161}},
Month = {{MAR}},
Abstract = {{Little research has examined the impacts of enterprise resource planning
   (ERP) systems implementation on job satisfaction. Based on a 12-month
   study of 2,794 employees in a telecommunications firm, we found that ERP
   system implementation moderated the relationships between three job
   characteristics (skill variety, autonomy, and feedback) and job
   satisfaction. Our findings highlight the key role that ERP system
   implementation can have in altering well established relationships in
   the context of technology-enabled organizational change situations. This
   work also extends research on technology diffusion by moving beyond a
   focus on technology-centric outcomes, such as system use, to
   understanding broader job outcomes.}},
