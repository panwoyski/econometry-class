@article{ ISI:000180346100005,
Author = {Mandal, P and Gunasekaran, A},
Title = {{Issues in implementing ERP: A case study}},
Journal = {{EUROPEAN JOURNAL OF OPERATIONAL RESEARCH}},
Year = {{2003}},
Volume = {{146}},
Number = {{2}},
Pages = {{274-283}},
Month = {{APR 16}},
Abstract = {{Information technology/systems play a major role in improving the
   competitiveness of organizations. There are numerous enterprise
   information software packages available in the market. One of them is
   enterprise resource planning (ERP). ERP can provide significant
   improvements in efficiency across a company, but only when implemented
   correctly. Otherwise, an ERP system could be a curse and drag the whole
   enterprise into spiraling inefficiency. Planning for ERP systems and
   their implementations requires an integrated approach to meet the
   requirements of various functional areas. With a brief overview of ERP
   implementations, this paper describes some experiences of an ERP
   implementation in a water corporation. The case study reveals some of
   the intricacies during the planning and implementation stages that may
