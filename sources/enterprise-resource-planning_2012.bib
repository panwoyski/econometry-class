@article{ ISI:000306122500003,
Author = {Remus, Ulrich},
Title = {{Exploring the Dynamics behind Knowledge Management Challenges-An
   Enterprise Resource Planning Case Study}},
Journal = {{INFORMATION SYSTEMS MANAGEMENT}},
Year = {{2012}},
Volume = {{29}},
Number = {{3}},
Pages = {{188-200}},
Abstract = {{The conductor of this case study uses causal mapping to show that
   external changes to an enterprise resource planning project can trigger
   a chain reaction, resulting in weak performance of key knowledge
   management activities such as knowledge capture, sharing, and
   integration. Management decisions responding to these changes may lead
   to knowledge dilemmas that can trigger unwanted dynamic behavior,
   finally causing project drift. This research extends existing knowledge
   on knowledge management challenges by emphasizing the need to reveal the
   dynamics behind how knowledge management challenges unfold over time.}},
