@inproceedings{ ISI:000342902300052,
Author = {Holmberg, Nicklas and Johansson, Bjorn},
Editor = {{Bajec, M and Eder, J}},
Title = {{Viewing Enterprise Resource Planning Systems as Services: A Conceptual
   View, Based on Practical Experiences, of Designing Information Systems
   as Services}},
Booktitle = {{ADVANCED INFORMATION SYSTEMS ENGINEERING WORKSHOPS, CAISE 2012}},
Series = {{Lecture Notes in Business Information Processing}},
Year = {{2012}},
Volume = {{112}},
