@article{ ISI:000326885700003,
Author = {Sierra, Laura and Zorio, Ana and Garcia-Benau, Maria A.},
Title = {{Sustainable Development and Assurance of Corporate Social Responsibility
   Reports Published by Ibex-35 Companies}},
