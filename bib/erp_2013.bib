@article{ ISI:000314179800003,
Author = {Kurapatskie, Brent and Darnall, Nicole},
Title = {{Which Corporate Sustainability Activities are Associated with Greater
   Financial Payoffs?}},
Journal = {{BUSINESS STRATEGY AND THE ENVIRONMENT}},
Year = {{2013}},
Volume = {{22}},
Number = {{1}},
Pages = {{49-61}},
Month = {{JAN}},
Abstract = {{While many company managers and academic researchers have argued that
   businesses that develop a sustainability focus also may improve their
   financial performance, little information is known about whether firms'
   different types of sustainability activities are related to varying
   degrees of financial gain. This paper assesses the economic relationship
   between two types of sustainability activities lower- and higher-order
   derived from the sustainability value framework of Hart and Milstein
