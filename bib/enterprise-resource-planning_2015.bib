@article{ ISI:000348452500021,
Author = {Chang, Betty and Kuo, Chin and Wu, Chih-Hung and Tzeng, Gwo-Hshiung},
Title = {{Using Fuzzy Analytic Network Process to assess the risks in enterprise
   resource planning system implementation}},
