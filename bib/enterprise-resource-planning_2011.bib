Title = {{The Evaluation Model of Entrepreneurial Strategic Leadership on the
   Sustainable Growth of Resource-Based Enterprises-Based on AHP and Fuzzy
   Synthetically Evaluation}},
Booktitle = {{ENVIRONMENT, LOW-CARBON AND STRATEGY}},
Year = {{2011}},
Pages = {{590-597}},
Note = {{International Conference on Strategic Management (ICSM 2011), Phuket,
   THAILAND, DEC 21-23, 2011}},
Abstract = {{Entrepreneurial strategic leadership is the most key factor considered
   by many foreign scholars in the continuously growing enterprises.
   Because resources are limited, the growth of enterprise depends largely
   on the entrepreneurial strategic plan about natural resources and the
   improving of the entrepreneurial strategic leadership. This paper is
   based on the resource-based enterprises, and uses the AHP to calculate
   the weight of the indexes, then assesses the indexes by fuzzy
   synthetical evaluation. It offers a method for scientific and reasonable
