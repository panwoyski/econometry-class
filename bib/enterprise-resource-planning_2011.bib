@article{ ISI:000286088800001,
Author = {Van Nieuwenhuyse, Inneke and De Boeck, Liesje and Lambrecht, Marc and
   Vandaele, Nico J.},
Title = {{Advanced resource planning as a decision support module for ERP}},
Journal = {{COMPUTERS IN INDUSTRY}},
Year = {{2011}},
Volume = {{62}},
Number = {{1}},
Pages = {{1-8}},
Month = {{JAN}},
Abstract = {{The planning and decision support capabilities of the manufacturing
   planning and control system, which provides the core of any enterprise
   resource planning package, can be enhanced substantively by the
   inclusion of a decision support module as an add-on at the midterm
   planning level. This module, called advanced resource planning (ARP),
   provides a parameter-setting process, with the ultimate goal of yielding
   realistic information about production lead times for scheduling
   purposes, sales and marketing, strategic and operational decision
   making, and suppliers and customers. This article illustrates the ARP
   approach with reports from several real-life implementations by large
   industrial companies. (C) 2010 Elsevier B.V. All rights reserved.}},
