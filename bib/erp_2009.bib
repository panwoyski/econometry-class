@article{ ISI:000207902800002,
Author = {Wagner, Marcus},
Title = {{Innovation and Competitive Advantages from the Integration of Strategic
   Aspects with Social and Environmental Management in European Firms}},
Journal = {{BUSINESS STRATEGY AND THE ENVIRONMENT}},
Year = {{2009}},
Volume = {{18}},
Number = {{5}},
Pages = {{291-306}},
Month = {{JUL}},
Abstract = {{This paper analyses the nature and details of the association that the
   integration of social and environmental considerations with corporate
   strategy has for different competitive advantages and innovation
   activities at the firm level. Its objective is to answer the question as
   to whether a positive link exists between integration and the effects of
   environmental and social performance on these different dimensions of
   economic performance. The question of the specific form of this
   relationship is also raised. These questions are analysed using cluster
   analysis and regression models. Results are presented for four different
   dimensions of competitive advantage, and for two types of innovation
   activity. These raise the possibility that the process of integration is
   more important for bringing about a positive link than a resulting
   integration type. Copyright (C) 2007 John Wiley \& Sons, Ltd and ERP
   Environment.}},
