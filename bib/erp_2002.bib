@article{ ISI:000179429400005,
Author = {Ardekani, BA and Choi, SJ and Hossein-Zadeh, GA and Porjesz, B and
   Tanabe, JL and Lim, KO and Bilder, R and Helpern, JA and Begleiter, H},
Title = {{Functional magnetic resonance imaging of brain activity in the visual
   oddball task}},
