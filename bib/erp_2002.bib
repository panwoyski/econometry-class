
@article{ ISI:000178630600012,
Author = {Molholm, S and Ritter, W and Murray, MM and Javitt, DC and Schroeder, CE
   and Foxe, JJ},
Title = {{Multisensory auditory-visual interactions during early sensory
   processing in humans: a high-density electrical mapping study}},
Journal = {{COGNITIVE BRAIN RESEARCH}},
Year = {{2002}},
