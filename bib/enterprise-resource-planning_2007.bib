@inproceedings{ ISI:000250866200004,
Author = {Moe, Carl Erik and Fosser, Erik and Leister, Ole Henrik and Newman, Mike},
Editor = {{Magyar, G and Knapp, G}},
Title = {{How can organizations achieve competitive advantages using ERP systems
   through managerial processes?}},
Booktitle = {{ADVANCES IN INFORMATION SYSTEMS DEVELOPMENT, VOL 1: NEW METHODS AND
   PRACTICE FOR THE NETWORKED SOCIETY}},
Year = {{2007}},
Pages = {{37+}},
Note = {{15th International Conference on Information Systems Development,
   Budapest, HUNGARY, AUG 31-SEP 02, 2006}},
Organization = {{Sci Assoc Infocommun; Natl Off Res \& Technol}},
Abstract = {{Enterprise Resource Planning (ERP) systems have become a standard for
   collecting all business areas in one system. For some organizations it
   is a necessity for doing business, while others use it to outperform
   competitors. Prior research in the area of ERP systems and competitive
   advantage is dispersed and insufficient. Based on literature review and
   in-depth interviews at two Norwegian enterprises we have developed a
   framework which we believe gives insight into the areas managers should
   focus on when implementing ERP systems.
   Our study supports the claim that ERP systems in themselves do not
   create a competitive advantage. Instead, they form an information basis
   which managers can utilize to outperform competitors. A good
   relationship between IT/IS managers and the rest of the management is
   crucial. The importance of top management support was highlighted and
   linking ERP and a data warehouse as a ``bolt-on{''} was very beneficial
   in one of the enterprises.}},
