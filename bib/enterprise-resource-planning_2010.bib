@inproceedings{ ISI:000280152500008,
Author = {Chen Weiya},
Editor = {{Zhang, Y}},
Title = {{Sustainable Development of Chinese Enterprises in Uncertain Economic
   Environment}},
Booktitle = {{PROCEEDINGS OF THE 6TH EURO-ASIA CONFERENCE ON ENVIRONMENT AND CSR:
   TECHNOLOGICAL INNOVATION AND MANAGEMENT SCIENCE SESSION, PT II}},
Year = {{2010}},
Pages = {{46-51}},
Note = {{6th Euro-Asia Conference on Environment and Corporate Social
   Responsibility, Istanbul, TURKEY, MAR 18-18, 2010}},
Organization = {{German Acad Exchange Serv Fdn; IMRE Alumni Network}},
Abstract = {{At present, increasingly more Chinese enterprises involved in
   international supply chains face with risk in the uncertain global
   economic environment. In order to achieve sustainable development,
   enterprises must take a lot of uncertainties into account in decision
   making and corporate management. In addition, enterprises should enhance
   quick response to consumer demands so as to reduce operational risk and
   realize maximum profits. In regard to human resource training and
   organizational structure, enterprises should have strong competitiveness
   and replace the old-style mechanical organizational structure with
   modern managerial systems. Under the conditions of complicated and
   diversified challenges, leaders of enterprises must enhance their own
   abilities in planning, judgment and control; promote transformation and
   innovation; and adopt flexible management. In addition, they should
   attach importance to systematic reforms and try to make their
   enterprises as ``learning organization{''} so as to build up core
   competence.}},
