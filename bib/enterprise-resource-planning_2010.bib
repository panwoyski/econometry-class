@article{ ISI:000276280500006,
Author = {McDonnell, Anthony and Lamare, Ryan and Gunnigle, Patrick and Lavelle,
   Jonathan},
Title = {{Developing tomorrow's leaders-Evidence of global talent management in
   multinational enterprises}},
