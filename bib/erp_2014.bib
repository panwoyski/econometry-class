
@article{ ISI:000342493400015,
Author = {Chae, Ho-Chang and Koh, Chang E. and Prybutok, Victor R.},
Title = {{INFORMATION TECHNOLOGY CAPABILITY AND FIRM PERFORMANCE: CONTRADICTORY
   FINDINGS AND THEIR POSSIBLE CAUSES}},
Journal = {{MIS QUARTERLY}},
