@article{ ISI:000167882900002,
Author = {Hahne, A and Jescheniak, JD},
Title = {{What's left if the Jabberwock gets the semantics? An ERP investigation
   into semantic and syntactic processes during auditory comprehension}},
Journal = {{COGNITIVE BRAIN RESEARCH}},
